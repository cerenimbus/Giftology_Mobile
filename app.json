{
  "expo": {
    "name": "ROR",
    "slug": "relationshipradar",
    "version": "1.0.3",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.cerenimbus.relationshipradar",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.cerenimbus.relationshipradar"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/applogo.png",
    "splash": {
      "image": "./assets/applogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "9244bc88-d1af-4b44-a6c5-f4ceab153e64"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "enableBitcode": false
          }
        }
      ]
    ],
    "owner": "cerenimbus"
  }
}
